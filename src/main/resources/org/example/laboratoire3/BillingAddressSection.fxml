<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>

<TitledPane xmlns:fx="http://javafx.com/fxml/1" fx:id="BillingAddressSection" collapsible="false" managed="true" text="Adresse de facturation" fx:controller="org.example.laboratoire3.BillingAddressSection">
    <GridPane alignment="CENTER_LEFT" hgap="10" vgap="10">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="150.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
        </columnConstraints>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
        <children>
            <Label text="Adresse de facturation:" />
            <TextField fx:id="BillingAddressTextField" GridPane.columnIndex="1" />
        </children>
        <rowConstraints>
            <RowConstraints />
        </rowConstraints>
    </GridPane>
</TitledPane>
